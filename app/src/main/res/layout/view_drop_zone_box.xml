<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:layout_height="@dimen/drop_zone_box_height"
    tools:layout_width="@dimen/drop_zone_box_width"
    tools:parentTag="android.widget.FrameLayout">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/drop_zone_box_internal_icon_margin_top"
        android:src="@drawable/man_happy"
        tools:ignore="ContentDescription" />

    <fr.tvbarthel.apps.shapi.shape.ShapeView
        android:id="@+id/view_drop_zone_box_internal_shape"
        android:layout_width="@dimen/drop_zone_box_internal_shape_width"
        android:layout_height="@dimen/drop_zone_box_internal_shape_height"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/drop_zone_box_internal_shape_margin_top"
        android:padding="@dimen/drop_zone_box_internal_shape_padding" />

</merge>